<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.60" fx:controller="Fenetres.MainWindowController">
  <rowConstraints>
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" percentHeight="70.0"/>
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />    
  </rowConstraints>
  <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />   
  </columnConstraints>  
  <ListView prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="0" />
  <ListView prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="1" />
  <ListView prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="2" />
  <Button GridPane.columnIndex="0" GridPane.rowIndex="1" GridPane.halignment="CENTER" text="Ajouter capteur" onAction="#AjouterCapteurAlgo"/>
  <Button GridPane.columnIndex="0" GridPane.rowIndex="2" GridPane.halignment="CENTER" text="Supprimer capteur" onAction="#SupprimerCapteurAlgo"/>
