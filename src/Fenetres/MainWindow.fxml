  <Button GridPane.columnIndex="1" GridPane.rowIndex="1" GridPane.halignment="CENTER" text="Ajouter méga-capteur" onAction="#AjouterMegaCapteur"/>
  <Button GridPane.columnIndex="1" GridPane.rowIndex="2" GridPane.halignment="CENTER" text="Supprimer méga-capteur" onAction="#SupprimerMegaCapteur"/>
  <Button GridPane.columnIndex="1" GridPane.rowIndex="3" GridPane.halignment="CENTER" text="Modifier méga-capteur" onAction="#ModifierMegaCapteur"/>
  <Button GridPane.columnIndex="2" GridPane.rowIndex="1" GridPane.halignment="CENTER" text="Ajouter fenêtre" onAction="#AjouterFenetre"/>
  <Button GridPane.columnIndex="2" GridPane.rowIndex="2" GridPane.halignment="CENTER" text="Supprimer fenêtre" onAction="#SupprimerFenetre"/>
  <Button GridPane.columnIndex="2" GridPane.rowIndex="3" GridPane.halignment="CENTER" text="Modifier fenêtre" onAction="#ModifierFenetre"/>
</GridPane>
