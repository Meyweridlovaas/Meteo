<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane id="GridPane" prefHeight="100.0" prefWidth="280.0" xmlns:fx="http://javafx.com/fxml/1" > 
  <rowConstraints>   
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />        
  </rowConstraints>
  <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />   
  </columnConstraints>
  <ChoiceBox GridPane.columnSpan="2" GridPane.halignment="CENTER" fx:id="cbFenetre" />   
  <Button GridPane.rowIndex="1" GridPane.halignment="CENTER" text="Ok" onAction="#ok"/>
  <Button GridPane.columnIndex="1" GridPane.rowIndex="1" GridPane.halignment="CENTER" text="Annuler" onAction="#annuler"/>
</GridPane>
